{% extends 'basefront.html.twig' %}

{% block title %}Etablissement{% endblock %}

{% block body %}

<div class="container">
    <h1>Etablissement</h1>
    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ etablissement.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ etablissement.Nom }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ etablissement.adresse }}</td>
            </tr>
            <tr>
                <th>Num_contact</th>
                <td>{{ etablissement.getNumContact() }}</td>
            </tr>

            <tr>
                
            </tr>
        </tbody>
    </table>

    <a class="btn btn-secondary" href="{{ path('app_etablissement_index') }}">back to list</a>

    <a class="btn btn-primary" href="{{ path('app_etablissement_edit', {'id': etablissement.id}) }}">edit</a>
    
    <div style="display: inline;width:200px">
    {{ include('etablissement/_delete_form.html.twig') }}
    </div>
</div>

{% endblock %}
