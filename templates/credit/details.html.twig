<table class="table">
    <tbody>
    <tr>
        <th>Id</th>
        <td>{{ credit.id }}</td>
    </tr>
    <tr>
        <th>Montant</th>
        <td>{{ credit.Montant }}</td>
    </tr>
    <tr>
        <th>MontantEmprunte</th>
        <td>{{ credit.MontantEmprunte }}</td>
    </tr>
    <tr>
